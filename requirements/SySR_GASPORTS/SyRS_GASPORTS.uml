  <Requirements:Requirement xmi:id="_KxJzsglzEeqiyaaQ4dgl3Q" base_Class="_KxJMkAlzEeqiyaaQ4dgl3Q" id="RFUG02" text="The UGV must estimate its battery state all the time."/>
  <Requirements:Requirement xmi:id="_RmGSRAlzEeqiyaaQ4dgl3Q" base_Class="_RmFrIAlzEeqiyaaQ4dgl3Q" id="RFUG03" text="The UGV must be able to avoid all the obstacles on his trajectory."/>
  <Requirements:Requirement xmi:id="_WSAArwlzEeqiyaaQ4dgl3Q" base_Class="_WR-ygAlzEeqiyaaQ4dgl3Q" id="RFUG04" text="The UGV must be able to turn back to the initial point if it will be necessary."/>
  <Requirements:Requirement xmi:id="_beRRQQlzEeqiyaaQ4dgl3Q" base_Class="_beQDEAlzEeqiyaaQ4dgl3Q" id="RFUG05" text="The UGV must be able to send all the information about his position, battery level, speed and sensor lectures to the server."/>
  <Requirements:Requirement xmi:id="_jjo13glzEeqiyaaQ4dgl3Q" base_Class="_jjnnsAlzEeqiyaaQ4dgl3Q" id="RFUG06" text="The UGV must be able to receive the information about his trajectory and new values for the actuators."/>
  <Requirements:Requirement xmi:id="_pTZ1AAlzEeqiyaaQ4dgl3Q" base_Class="_pTYm4AlzEeqiyaaQ4dgl3Q" id="RFUG07" text="The UGV must be able to control his acceleration and velocity depend of the operation that it is performing."/>
  <Requirements:Requirement xmi:id="_v1tEYAlzEeqiyaaQ4dgl3Q" base_Class="_v1rPMAlzEeqiyaaQ4dgl3Q" id="RFUG08 " text="The UGV must be able to take the balls of the court."/>
  <Requirements:Requirement xmi:id="_0nGW0AlzEeqiyaaQ4dgl3Q" base_Class="_0nEhoAlzEeqiyaaQ4dgl3Q" id="RFUG09 " text="The UGV must be able to leave the balls in a basket. "/>
  <Requirements:Requirement xmi:id="_B-RMQAl0EeqiyaaQ4dgl3Q" base_Class="_B-PXEAl0EeqiyaaQ4dgl3Q" id="RFUG10" text="The UGV must be able to ride along a path that the server sent."/>
  <Requirements:Requirement xmi:id="_LYGpggl0EeqiyaaQ4dgl3Q" base_Class="_LYFbUAl0EeqiyaaQ4dgl3Q" id="RFUG11" text="The UGV must be able to ride to a point without a trajectory stablished before."/>
  <Requirements:Requirement xmi:id="_QYgBMAl0EeqiyaaQ4dgl3Q" base_Class="_QYezEAl0EeqiyaaQ4dgl3Q" id="RFUG12" text="The UGV must detect obstacles on his trajectory."/>
  <Requirements:DeriveReqt xmi:id="_oSKlAAl0EeqiyaaQ4dgl3Q" base_DirectedRelationship="_oSJW4Al0EeqiyaaQ4dgl3Q" base_Abstraction="_oSJW4Al0EeqiyaaQ4dgl3Q"/>
  <Blocks:Block xmi:id="_THcBwAl2EeqiyaaQ4dgl3Q" base_Class="_THazoAl2EeqiyaaQ4dgl3Q"/>
  <Requirements:Requirement xmi:id="_b9vUEAl2EeqiyaaQ4dgl3Q" base_Class="_b9te4Al2EeqiyaaQ4dgl3Q" id="RFSE01" text="The server must be able to send all the information necessary for the control of the UAV and the UGV."/>
  <Requirements:Requirement xmi:id="_g_5mYAl2EeqiyaaQ4dgl3Q" base_Class="_g_4YQAl2EeqiyaaQ4dgl3Q" id="RFSE02" text="The server must be able to receive the information about the status of the UAV and the UGV."/>
  <Requirements:Requirement xmi:id="_iXlLAAl2EeqiyaaQ4dgl3Q" base_Class="_iXjV0Al2EeqiyaaQ4dgl3Q" id="RFSE03" text="The server must be able to start all the system."/>
  <Requirements:Requirement xmi:id="_j5jUEAl2EeqiyaaQ4dgl3Q" base_Class="_j5he4Al2EeqiyaaQ4dgl3Q" id="RFSE04" text="The server must be able to stop all the system if it was necessary."/>
  <Requirements:Requirement xmi:id="_mCCqkAl2EeqiyaaQ4dgl3Q" base_Class="_mCBccAl2EeqiyaaQ4dgl3Q" id="RFSE05" text="The server must be able to restart all the system if it was necessary."/>
  <Requirements:Requirement xmi:id="_o-75gAl2EeqiyaaQ4dgl3Q" base_Class="_o-6EUAl2EeqiyaaQ4dgl3Q" id="RFSE06" text="The server must be able to process all the information received by the UGV."/>
  <Requirements:Requirement xmi:id="_sEsJgAl2EeqiyaaQ4dgl3Q" base_Class="_sEq7YAl2EeqiyaaQ4dgl3Q" id="RFSE07" text="The server must be able to process all the information received by the UAV."/>
  <Requirements:Requirement xmi:id="_w3XOUAl2EeqiyaaQ4dgl3Q" base_Class="_w3VZIAl2EeqiyaaQ4dgl3Q" id="RFSE08" text="The server must be able to estimate the UGV trajectory."/>
  <Requirements:Requirement xmi:id="_YDPX8Al3EeqiyaaQ4dgl3Q" base_Class="_YDNiwAl3EeqiyaaQ4dgl3Q" id="RFSE09" text="The server must be able to estimate the UAV trajectory."/>
  <Requirements:Requirement xmi:id="_ZXz-MAl3EeqiyaaQ4dgl3Q" base_Class="_ZXyJAAl3EeqiyaaQ4dgl3Q" id="RFSE10" text="The server must be including a user-friendly interface to interact with the system."/>
  <Blocks:Block xmi:id="_UBlMEA4nEeq-06HUUDh6BQ" base_Class="_UBaM8A4nEeq-06HUUDh6BQ"/>
  <Requirements:Requirement xmi:id="_fh8Q4A4nEeq-06HUUDh6BQ" base_Class="_fh4mgA4nEeq-06HUUDh6BQ" id="RNFPE01" text="The UAV shall detect the ball locate with an accuracy of 3 cm."/>
  <Requirements:Requirement xmi:id="_HJjUQA4oEeq-06HUUDh6BQ" base_Class="_HJfC0A4oEeq-06HUUDh6BQ" id="RNFPE02 " text="The communication between the server, UAV and UGV must be constant during the execution of the mission."/>
  <Requirements:Requirement xmi:id="_Oa-TcA4oEeq-06HUUDh6BQ" base_Class="_Oa73MA4oEeq-06HUUDh6BQ" id="RNFPE03 " text="The UAVs must be several batteries to work. Concretely, there must be batteries for a maximum of one hour."/>
  <Requirements:Requirement xmi:id="_XgIhIA4oEeq-06HUUDh6BQ" base_Class="_XgFd0A4oEeq-06HUUDh6BQ" id="RNFPE04" text="The system should have enough impact resistance to ball impacts in the court. The materials must be the proper ones and should last during the life cycle of the product."/>
  <Requirements:Requirement xmi:id="_evJskA4oEeq-06HUUDh6BQ" base_Class="_evHQUA4oEeq-06HUUDh6BQ" id="RNFPE05 " text="Control server must estimate UAV position and orientation with less than 3% error."/>
  <Requirements:Requirement xmi:id="_jqx5EA4oEeq-06HUUDh6BQ" base_Class="_jqvc0A4oEeq-06HUUDh6BQ" id="RNFPE06 " text="Control server must be able to control UAV with position commands with less than 5% error."/>
  <Requirements:Requirement xmi:id="_wPOREA4oEeq-06HUUDh6BQ" base_Class="_wPL00A4oEeq-06HUUDh6BQ" id="RNFPE07 " text="Control server must be able to control UAV with velocity commands with less than 5% error."/>
  <Requirements:Requirement xmi:id="_1ZDRoA4oEeq-06HUUDh6BQ" base_Class="_1ZBccA4oEeq-06HUUDh6BQ" id="RNFPE08 " text="UAV must be able to keep its current state when no commands were received within 0.5 seconds."/>
  <Requirements:Requirement xmi:id="_5q3sIA4oEeq-06HUUDh6BQ" base_Class="_5q128A4oEeq-06HUUDh6BQ" id="RNFPE09 " text="UAV must be able to communicate with control server with a bit error rate of 10^-9 or less."/>
  <Requirements:Requirement xmi:id="_AOnF8A4pEeq-06HUUDh6BQ" base_Class="_AOlQwA4pEeq-06HUUDh6BQ" id="RNFPE10 " text="Control server must estimate UGV position and orientation with less than 3% error."/>
  <Requirements:Requirement xmi:id="_DsPqcA4pEeq-06HUUDh6BQ" base_Class="_DsN1QA4pEeq-06HUUDh6BQ" id="RNFPE11 " text="Control server must be able to control UGV with position commands with less than 5% error."/>
  <Requirements:Requirement xmi:id="_IgxwcA4pEeq-06HUUDh6BQ" base_Class="_Igv7QA4pEeq-06HUUDh6BQ" id="RNFPE12 " text="Control server must be able to control UGV with velocity commands with less than 5% error."/>
  <Requirements:Requirement xmi:id="_NjPkwA4pEeq-06HUUDh6BQ" base_Class="_NjNvkA4pEeq-06HUUDh6BQ" id="RNFPE13 " text="UGV must be able to keep its current state when no commands were received within 0.5 seconds."/>
  <Requirements:Requirement xmi:id="_Q_r2cA4pEeq-06HUUDh6BQ" base_Class="_Q_qBQA4pEeq-06HUUDh6BQ" id="RNFPE14 " text="UGV must be able to communicate with control server with a bit error rate of 10^-9 or less."/>
  <Blocks:Block xmi:id="_e8TQYA4rEeq-06HUUDh6BQ" base_Class="_e8Q0IA4rEeq-06HUUDh6BQ"/>
  <Requirements:Requirement xmi:id="_sCMSIA43Eeq-06HUUDh6BQ" base_Class="_sCKc8A43Eeq-06HUUDh6BQ" id="RNFSE01 " text="An authorization for operating the drone-vehicle system should be granted by ETSII´s authorities before testing the system on the field."/>
  <Requirements:Requirement xmi:id="_4DgH4A43Eeq-06HUUDh6BQ" base_Class="_4DeSsA43Eeq-06HUUDh6BQ" id="RNFSE02 " text="If the UAV remains more than 10 seconds over the wrong side of the tennis court (in which the player is training), the drone shall land."/>
  <Requirements:Requirement xmi:id="_BX0y8A44Eeq-06HUUDh6BQ" base_Class="_BXy9wA44Eeq-06HUUDh6BQ" id="RNFSE03 " text="Pilots/Operators are encouraged to read the system’s manual thoroughly to become familiar with all features, limitations, and recommended maintenance."/>
  <Requirements:Requirement xmi:id="_GBklQA44Eeq-06HUUDh6BQ" base_Class="_GBiwEA44Eeq-06HUUDh6BQ" id="RNFSE04 " text="The system shall include a pre-operational safety check."/>
  <Requirements:Requirement xmi:id="_L2vo4A44Eeq-06HUUDh6BQ" base_Class="_L2tzsA44Eeq-06HUUDh6BQ" id="RNFSE05 " text="The system shall operate during daylight hours."/>
  <Requirements:Requirement xmi:id="_RFNR0A44Eeq-06HUUDh6BQ" base_Class="_RFMDsA44Eeq-06HUUDh6BQ" id="RNFSE06 " text="Do not fly the unmanned aircraft system over crowds of individuals."/>
  <Requirements:Requirement xmi:id="_XqnfkA44Eeq-06HUUDh6BQ" base_Class="_XqlqYA44Eeq-06HUUDh6BQ" id="RNFSE07 " text="UAV and UGV are properly maintained for safe operation and kept in a secure location when not in use."/>
  <Requirements:Requirement xmi:id="_dDWe0A44Eeq-06HUUDh6BQ" base_Class="_dDVQsA44Eeq-06HUUDh6BQ" id="RNFSE08 " text="System must comply with current legal standards (Real Decreto 1036/2017) and any other commitments that the customer has acquired at all times."/>
  <Requirements:Requirement xmi:id="_iqDBkA44Eeq-06HUUDh6BQ" base_Class="_iqBzcA44Eeq-06HUUDh6BQ" id="RNFSE09 " text="System shall pursue sustainable development objectives (ODS) specially the ones regarding health and well-being, sustainable cities, industry and innovation and responsible production and consumption."/>
  <Requirements:Requirement xmi:id="_njdnkA44Eeq-06HUUDh6BQ" base_Class="_njcZcA44Eeq-06HUUDh6BQ" id="RNFSE10 " text="The UAV must emit a kind of noise in order to the user can switch off the UAV and change the battery or reload it."/>
  <Requirements:Requirement xmi:id="_sD7iEA44Eeq-06HUUDh6BQ" base_Class="_sD5s4A44Eeq-06HUUDh6BQ" id="RNFSE11 " text="The user must have all the licenses to use the UAVs within a city."/>
  <Requirements:Requirement xmi:id="_xbzlsA44Eeq-06HUUDh6BQ" base_Class="_xbxwgA44Eeq-06HUUDh6BQ" id="RNFSE12 " text="The flying height of UAV cannot exceed 120 meters, due to the weight of UAV is 500 grams and this height is set by law."/>
  <Requirements:Requirement xmi:id="_2tfhwA44Eeq-06HUUDh6BQ" base_Class="_2teToA44Eeq-06HUUDh6BQ" id="RNFSE13 " text="To recharge the battery, the UAV must be completely stopped so as not to damage the user or be damaged."/>
  <Requirements:Requirement xmi:id="_-xPVwA44Eeq-06HUUDh6BQ" base_Class="_-xNgkA44Eeq-06HUUDh6BQ" id="RNFSE14 " text="Fly at a minimum distance of 8 km from any airport or airfield."/>
  <Requirements:Requirement xmi:id="_EGUkEA45Eeq-06HUUDh6BQ" base_Class="_EGTV8A45Eeq-06HUUDh6BQ" id="RNFSE15 " text="It must be ensured that during training the UGV is always on the side of the track that the tennis player is not."/>
  <Requirements:Requirement xmi:id="_MyseUA45Eeq-06HUUDh6BQ" base_Class="_MyqpIA45Eeq-06HUUDh6BQ" id="RNFSE16 " text="In order to ensure that the user visualizes the system in every moment, the colour of the system should be bright enough to not hide it in the environment where it will be used."/>
  <Requirements:Requirement xmi:id="_mHpUYA45Eeq-06HUUDh6BQ" base_Class="_mHnfMA45Eeq-06HUUDh6BQ" id="RNFSE17 " text="In case there is a situation in which the system could harm the user, an emergency alert should be displayed to inform about it and avoid accidents."/>
  <Requirements:Requirement xmi:id="_neFi4A45Eeq-06HUUDh6BQ" base_Class="_neDtsA45Eeq-06HUUDh6BQ" id="RNFSE18" text="The UGV must be able to execute a safe stop whenever the associated signal is sent by the final user via the server interface."/>
  <Blocks:Block xmi:id="_kkxW8A46Eeq-06HUUDh6BQ" base_Class="_kkvhwA46Eeq-06HUUDh6BQ"/>
  <Requirements:Requirement xmi:id="_pAc4EA46Eeq-06HUUDh6BQ" base_Class="_pAbC4A46Eeq-06HUUDh6BQ" id="RNFER01 " text="In case of failure of the connection between the UAV and the server for more than 5 seconds, the drone shall land."/>
  <Requirements:Requirement xmi:id="_qKGAsA46Eeq-06HUUDh6BQ" base_Class="_qKELgA46Eeq-06HUUDh6BQ" id="RNFER02 " text="In case of problematic weather conditions, such as heavy wind, that make the UAV stay away from its path three times or more, the drone shall land."/>
  <Requirements:Requirement xmi:id="_rFwXsA46Eeq-06HUUDh6BQ" base_Class="_rFvJkA46Eeq-06HUUDh6BQ" id="RNFER03 " text="UGV must have a maximum pick up time. In case the allowed time is exceeded, the UGV must skip that ball pickup and go for the next one."/>
  <Blocks:Block xmi:id="_HLlZ8A47Eeq-06HUUDh6BQ" base_Class="_HLjkwA47Eeq-06HUUDh6BQ"/>
  <Requirements:Requirement xmi:id="_QVC40A47Eeq-06HUUDh6BQ" base_Class="_QVBDoA47Eeq-06HUUDh6BQ" id="RNFUS01 " text="The artificial lightning should not interfere with the recognition system of the drone for locating the tennis balls in the court."/>
  <Requirements:Requirement xmi:id="_W32egA47Eeq-06HUUDh6BQ" base_Class="_W30pUA47Eeq-06HUUDh6BQ" id="RNFUS02 " text="A user manual should be made so that all the users of the system understand the way the system works."/>
  <Requirements:Requirement xmi:id="_Ygw9EA47Eeq-06HUUDh6BQ" base_Class="_YgvH4A47Eeq-06HUUDh6BQ" id="RFNUS03 " text="If the UGV is too heavy, it must be transported in a kind of platform. This platform must be easy to use by the user."/>
  <Requirements:Requirement xmi:id="_avgFgA47Eeq-06HUUDh6BQ" base_Class="_aveQUA47Eeq-06HUUDh6BQ" id="RNFUS04" text="The server used to exchange the information between the UAV and the UGV must be easy to use and transport, such as a laptop."/>
  <Requirements:Requirement xmi:id="_h3TOoA47Eeq-06HUUDh6BQ" base_Class="_h3RZcA47Eeq-06HUUDh6BQ" id="RNFUS05 " text="The system should include indicators in order to inform the user about its status."/>
  <Requirements:Requirement xmi:id="_jk71IA47Eeq-06HUUDh6BQ" base_Class="_jk5_8A47Eeq-06HUUDh6BQ" id="RNFUS06 " text="The system controller must have a simple and complete interface."/>
  <Blocks:Block xmi:id="_KFtMQA48Eeq-06HUUDh6BQ" base_Class="_KFqwAA48Eeq-06HUUDh6BQ"/>
  <Requirements:Requirement xmi:id="_Nxp5kA48Eeq-06HUUDh6BQ" base_Class="_NxoEYA48Eeq-06HUUDh6BQ" id="RNFRE01 " text="The final price of the system should be stablished so that it will be easily affordable by tennis institutions/corporations."/>
</xmi:XMI>
