  <Requirements:Requirement xmi:id="_sJS2UAluEeqiyaaQ4dgl3Q" base_Class="_sJRBIAluEeqiyaaQ4dgl3Q" id="RFUA01" text="The UAV must take off vertically when the systems starts working."/>
  <Requirements:Requirement xmi:id="_1hQEQgluEeqiyaaQ4dgl3Q" base_Class="_1hO2EAluEeqiyaaQ4dgl3Q" id="RFUA02" text="The UAV must estimate its flight height all the time."/>
  <Requirements:Requirement xmi:id="_76uu0gluEeqiyaaQ4dgl3Q" base_Class="_76tgoAluEeqiyaaQ4dgl3Q" id="RFUA03" text="The UAV must estimate its position all the time."/>
  <Requirements:Requirement xmi:id="_AMGdUAlvEeqiyaaQ4dgl3Q" base_Class="_AMFPMAlvEeqiyaaQ4dgl3Q" id="RFUA04" text="The UAV must estimate its battery state all the time."/>
  <Requirements:Requirement xmi:id="_HY3mEglvEeqiyaaQ4dgl3Q" base_Class="_HY2X4AlvEeqiyaaQ4dgl3Q" id="RFUA05" text="The UAV must be able to fly along a path that the server sent."/>
  <Requirements:Requirement xmi:id="_Wg2RwAlvEeqiyaaQ4dgl3Q" base_Class="_Wg0ckAlvEeqiyaaQ4dgl3Q" id="RFUA06" text="The UAV must be able to fly to a point without a trajectory stablished before."/>
  <Requirements:Requirement xmi:id="_d4TngAlvEeqiyaaQ4dgl3Q" base_Class="_d4SZYAlvEeqiyaaQ4dgl3Q" id="RFUA07" text="The UAV must be able to avoid all the obstacles that it encounters in this trajectory."/>
  <Requirements:Requirement xmi:id="_h4jJ4QlvEeqiyaaQ4dgl3Q" base_Class="_h4h7sAlvEeqiyaaQ4dgl3Q" id="RFUA08" text="The UAV must be able to land on designated places."/>
  <Requirements:Requirement xmi:id="_llR6UglvEeqiyaaQ4dgl3Q" base_Class="_llQsIAlvEeqiyaaQ4dgl3Q" id="RFUA09" text="The UAV must be able to realize a safe auto land if it was necessary."/>
  <Requirements:Requirement xmi:id="_sT4gQAlvEeqiyaaQ4dgl3Q" base_Class="_sT3SIAlvEeqiyaaQ4dgl3Q" id="RFUA10" text="The UAV must detect obstacles on his trajectory."/>
  <Requirements:Requirement xmi:id="_2iebQglvEeqiyaaQ4dgl3Q" base_Class="_2idNEAlvEeqiyaaQ4dgl3Q" id="RFUA11" text="The UAV must detect tennis balls on the court."/>
  <Requirements:Requirement xmi:id="_CMUzMAlwEeqiyaaQ4dgl3Q" base_Class="_CMTlEAlwEeqiyaaQ4dgl3Q" id="RFUA12" text="The UAV must estimate the global coordinates of the tennis ball on the court."/>
  <Requirements:DeriveReqt xmi:id="_JwLmMAlwEeqiyaaQ4dgl3Q" base_DirectedRelationship="_JwJxAAlwEeqiyaaQ4dgl3Q" base_Abstraction="_JwJxAAlwEeqiyaaQ4dgl3Q"/>
  <Requirements:Requirement xmi:id="_WLTqIAlwEeqiyaaQ4dgl3Q" base_Class="_WLScAAlwEeqiyaaQ4dgl3Q" id="RFUA13" text="The UAV must be able to turn back to the initial point if it will be necessary."/>
  <Requirements:Requirement xmi:id="_eV_MkAlwEeqiyaaQ4dgl3Q" base_Class="_eV9-cAlwEeqiyaaQ4dgl3Q" id="RFUA14" text="The UAV must be able to send all the information about his position, battery level, speed and sensor lectures to the server."/>
  <Requirements:Requirement xmi:id="_m3pEpAlwEeqiyaaQ4dgl3Q" base_Class="_m3odgAlwEeqiyaaQ4dgl3Q" id="RFUA15" text="The UAV must be able to receive the information about his trajectory and new values for the actuators."/>
  <Requirements:Requirement xmi:id="_wg5d0AlwEeqiyaaQ4dgl3Q" base_Class="_wg4PsAlwEeqiyaaQ4dgl3Q" id="RFUG16" text="The UAV must be able to control his acceleration and velocity depend of the operation that it is performing."/>
